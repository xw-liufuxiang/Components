    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint --no-fix",
    "lint:fix": "vue-cli-service lint",
    "lint:css": "stylelint \"src/**/*.{css,less,sass,scss,vue}\"",
    "lint:css:fix": "stylelint --fix \"src/**/*.{css,less,sass,scss,vue}\"",
