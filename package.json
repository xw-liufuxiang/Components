  "files": [
    ".vscode",
    "public",
    "scripts",
    "src",
    ".browserslistrc",
    ".env.development",
    ".env.production",
    ".eslintignore",
    ".eslintrc.js",
    ".gitattributes",
    ".gitignore",
    ".gitlab-ci.yml",
    ".markdownlint.json",
    ".npmrc",
    "babel.config.js",
    "package-lock.json",
    "postcss.config.js",
    "stylelint.config.js",
    "vue.config.js"
